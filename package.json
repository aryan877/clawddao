{
  "name": "clawddao",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --dir apps/web",
    "build": "next build apps/web --webpack",
    "start": "next start apps/web",
    "lint": "eslint",
    "agent:worker": "tsx --tsconfig apps/worker/tsconfig.json apps/worker/server/index.ts",
    "agent:worker:loop": "tsx --tsconfig apps/worker/tsconfig.json apps/worker/agent-worker.ts",
    "agent:worker:once": "tsx --tsconfig apps/worker/tsconfig.json apps/worker/agent-worker.ts --once",
    "agent:worker:dry-run": "tsx --tsconfig apps/worker/tsconfig.json apps/worker/agent-worker.ts --dry-run --once",
    "setup:devnet-realm": "tsx scripts/setup-devnet-realm.ts",
    "seed:demo": "DOTENV_CONFIG_PATH=.env.local tsx -r dotenv/config scripts/seed-demo.ts",
    "spacetimedb:up": "docker-compose up -d spacetimedb",
    "spacetimedb:down": "docker-compose down",
    "spacetimedb:logs": "docker-compose logs -f spacetimedb",
    "spacetimedb:reset": "docker-compose down && docker-compose up -d spacetimedb",
    "dev:full": "npm run spacetimedb:up && concurrently -k \"npm run dev\" \"npm run agent:worker\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:live": "LIVE_TEST=true vitest run --config vitest.live.config.ts",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:all": "vitest run && vitest run --config vitest.integration.config.ts",
    "test:all:live": "vitest run && vitest run --config vitest.integration.config.ts && LIVE_TEST=true vitest run --config vitest.live.config.ts"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.32.1",
    "@metaplex-foundation/mpl-core": "^1.7.0",
    "@metaplex-foundation/umi": "^1.5.1",
    "@metaplex-foundation/umi-bundle-defaults": "^1.5.1",
    "@modelcontextprotocol/sdk": "^1.27.1",
    "@privy-io/react-auth": "^3.14.1",
    "@privy-io/server-auth": "^1.32.5",
    "@solana-program/memo": "^0.11.0",
    "@solana/spl-token": "^0.4.9",
    "@solana/web3.js": "^1.98.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "express": "^4.21.0",
    "framer-motion": "^12.34.3",
    "governance-idl-sdk": "^0.0.4",
    "helius-sdk": "^2.2.2",
    "lucide-react": "^0.575.0",
    "next": "16.1.6",
    "openai": "^4.104.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "recharts": "^3.7.0",
    "socialfi": "^0.1.14",
    "spacetimedb": "^2.0.1",
    "tailwind-merge": "^3.5.0",
    "tsx": "^4.20.6",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/express": "^5",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/coverage-v8": "^4.0.18",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^4.0.18"
  }
}
